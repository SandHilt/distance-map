{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "rootDir": "./src",
        "outDir": "./build",
        "esModuleInterop": true,
        "strict": true,
        "sourceMap": true,
        "noImplicitAny": true
    },
    "include": [
        "./src/**/*.ts"
    ], "exclude": [
        "./node_modules",
        "./build"
    ]
}